{
  "printWidth": 80,
  "singleQuote": true,
  "semi": false,
  "trailingComma": "all",
  "proseWrap": "never",
  "jsxSingleQuote": false,
  "arrowParens": "always",
  "overrides": [
    {
      "files": [
        ".prettierrc",
        "*.ts",
        "*.tsx"
      ],
      "options": {
        "parser": "react"
      }
    }
  ]
}